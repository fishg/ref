{
  "name": "ref",
  "description": "Turn Buffer instances into \"pointers\"",
  "keywords": [
    "native",
    "buffer",
    "extensions",
    "c++",
    "pointer",
    "reference",
    "dereference",
    "type",
    "int",
    "long",
    "float",
    "double",
    "byte",
    "64"
  ],
  "version": "2.0.3",
  "license": "MIT",
  "author": "Nathan Rajlich <nathan@tootallnate.net> (http://tootallnate.net)",
  "repository": {
    "type": "git",
    "url": "git://github.com/TooTallNate/ref.git"
  },
  "main": "./lib/ref.js",
  "scripts": {
    "install" : "node-pre-gyp install --fallback-to-build --runtime=electron --target=8.3.1",
    "docs": "node docs/compile",
    "test": "mocha --gc-global --expose-gc --reporter spec --use_strict"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "debug": "*",
    "nan": "^2.13.2",
    "node-pre-gyp": "0.6.x"
  },
  "binary": {
    "module_name" : "binding",
    "module_path" : "./build/",
    "remote_path" : "./{module_name}/v{version}/{configuration}/",
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz",
    "host"        : "https://ym-public.oss-cn-shenzhen.aliyuncs.com/node_prebuild"
  }
}
